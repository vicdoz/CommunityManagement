<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">org.hsqldb.jdbcDriver</property>
        <property name="hibernate.connection.url">jdbc:hsqldb:hsql://localhost/comunidades</property>
        <property name="hibernate.connection.username">sa</property>
        <property name="hibernate.dialect">org.hibernate.dialect.HSQLDialect</property>

		<mapping resource="accesoAdatos/_ORM/PropietarioMapping.xml"/>
		<mapping resource="accesoAdatos/_ORM/InmuebleMapping.xml"/>				
		<mapping resource="accesoAdatos/_ORM/ReciboInmuebleMapping.xml"/>
		<mapping resource="accesoAdatos/_ORM/ComunidadMapping.xml"/>
		<mapping resource="accesoAdatos/_ORM/NotaInformativaMapping.xml"/>		
		<mapping resource="accesoAdatos/_ORM/FacturaMapping.xml"/>			
		<mapping resource="accesoAdatos/_ORM/LinFacMapping.xml"/>	
		<mapping resource="accesoAdatos/_ORM/ConceptoMapping.xml"/>	
		<mapping resource="accesoAdatos/_ORM/CartaMapping.xml"/>	


	

    </session-factory>
</hibernate-configuration>
